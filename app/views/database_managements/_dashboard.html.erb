<script>
	$(document).ready(function(){
		 if( ( $("#link_label_company").text() ).match(/true/g) ){
		 	$("#link_company").click();
		 }
		 if( ( $("#link_label_type").text() ).match(/true/g) ){
		 	$("#link_type").click();
		 }
		 if( ( $("#link_label_location").text() ).match(/true/g) ){
		 	$("#link_location").click();
		 }
		 if( ( $("#link_label_subject").text() ).match(/true/g) ){
		 	$("#link_subject").click();
		 }
		 if( ( $("#link_label_grade").text() ).match(/true/g) ){
		 	$("#link_grade").click();
		 }
		 if( ( $("#link_label_itskill").text() ).match(/true/g) ){
		 	$("#link_itskill").click();
		 }
		 if( ( $("#link_label_language").text() ).match(/true/g) ){
		 	$("#link_language").click();
