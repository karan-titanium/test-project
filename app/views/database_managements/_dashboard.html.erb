<label id="link_label_company" style="display: none"><%= @from_companies.present? %></label>
<label id="link_label_type" style="display: none"><%= @from_types.present? %></label>
<label id="link_label_location" style="display: none"><%= @from_locations.present? %></label>
<label id="link_label_subject" style="display: none"><%= @from_subjects.present? %></label>
